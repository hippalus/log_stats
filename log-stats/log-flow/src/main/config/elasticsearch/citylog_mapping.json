//PUT -XPUT 'http://localhost:9200/citylogs/_mapping/valid' -d '
{
    "valid": {
      "properties": {
        "LogLevel": {
          "type": "keyword"
        },
        "Message": {
          "type": "text"
        },
        "City": {
          "type": "keyword"
        },
        "Date": {
          "type": "date",
          "format": "yyyy-MM-dd HH:mm:ss.SSS"
        }
      }
    },
  "settings": {
    "index": {
      "number_of_shards": "1",
      "number_of_replicas": "1"
    }
  }
}